    "eslint": "^8.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@hapi/hapi": "^20.2.1",
    "@jwd-crypto-signals/common": "^1.0.6",
    "dotenv": "^10.0.0",
    "joi": "^17.5.0",
