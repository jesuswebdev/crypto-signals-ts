    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@jwd-crypto-signals/common": "^1.0.6",
    "dotenv": "^10.0.0",
